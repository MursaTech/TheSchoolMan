.section
	.row
		.col-lg-9.main-chart
			/ CUSTOM CHART START
			.border-head
				%h3 USER VISITS
			.custom-bar-chart
				%ul.y-axis
					%li
						%span 10.000
					%li
						%span 8.000
					%li
						%span 6.000
					%li
						%span 4.000
					%li
						%span 2.000
					%li
						%span 0
				.bar
					.title JAN
					.value.tooltips{"data-original-title" => "8.500", "data-placement" => "top", "data-toggle" => "tooltip"} 85%
				.bar
					.title FEB
					.value.tooltips{"data-original-title" => "5.000", "data-placement" => "top", "data-toggle" => "tooltip"} 50%
				.bar
					.title MAR
					.value.tooltips{"data-original-title" => "6.000", "data-placement" => "top", "data-toggle" => "tooltip"} 60%
				.bar
					.title APR
					.value.tooltips{"data-original-title" => "4.500", "data-placement" => "top", "data-toggle" => "tooltip"} 45%
				.bar
					.title MAY
					.value.tooltips{"data-original-title" => "3.200", "data-placement" => "top", "data-toggle" => "tooltip"} 32%
				.bar
					.title JUN
					.value.tooltips{"data-original-title" => "6.200", "data-placement" => "top", "data-toggle" => "tooltip"} 62%
				.bar
					.title JUL
					.value.tooltips{"data-original-title" => "7.500", "data-placement" => "top", "data-toggle" => "tooltip"} 75%
			/ custom chart end
			.row.mt
				/ SERVER STATUS PANELS
				.col-md-4.col-sm-4.mb
					.grey-panel.pn.donut-chart>
						.grey-header
							%h5 SERVER LOAD
						%canvas#serverstatus01{:height => "120", :width => "120"}
						:javascript
							var doughnutData = [
									{
										value: 70,
										color:"#FF6B6B"
									},
									{
										value : 30,
										color : "#fdfdfd"
									}
								];
								var myDoughnut = new Chart(document.getElementById("serverstatus01").getContext("2d")).Doughnut(doughnutData);
														.row
							.col-sm-6.col-xs-6.goleft
								%p
									Usage
									%br>/
									Increase:
							.col-sm-6.col-xs-6
								%h2 21%
					<! --/grey-panel -->
				/ /col-md-4
				.col-md-4.col-sm-4.mb
					.darkblue-panel.pn>
						.darkblue-header
							%h5 DROPBOX STATICS
						%canvas#serverstatus02{:height => "120", :width => "120"}
						:javascript
							var doughnutData = [
									{
										value: 60,
										color:"#1c9ca7"
									},
									{
										value : 40,
										color : "#f68275"
									}
								];
								var myDoughnut = new Chart(document.getElementById("serverstatus02").getContext("2d")).Doughnut(doughnutData);
														%p April 17, 2014
						%footer
							.pull-left
								%h5
									%i.fa.fa-hdd-o
									17 GB
							.pull-right
								%h5 60% Used
					<! -- /darkblue panel -->
				/ /col-md-4
				.col-md-4.col-sm-4.mb
					/ REVENUE PANEL
					.green-panel.pn
						.green-header
							%h5 REVENUE
						.chart.mt
							.sparkline{"data-data" => "[200,135,667,333,526,996,564,123,890,464,655]", "data-fill-color" => "", "data-height" => "75", "data-highlight-line-color" => "#fff", "data-line-color" => "#fff", "data-line-width" => "1", "data-resize" => "true", "data-spot-color" => "#fff", "data-spot-radius" => "4", "data-type" => "line", "data-width" => "90%"}
						%p.mt
							%b $ 17,980
							= succeed "Month" do
								%br/
							Income
				/ /col-md-4